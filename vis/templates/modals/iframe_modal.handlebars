<!-- Modal -->
<div id="iframe_modal" class="modal fade" role="dialog" tabindex='-1'>
  <div class="modal-dialog modal-lg" id="pdf-modal">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="btn btn-default pull-right" data-dismiss="modal">&times;</button>
      </div>
      <div class="modal-body text-center">
        <div id="spinner-iframe">
            <p class="wait-message">{{{ spinner_text }}}
            <p class="wait-spinner"><span id="spinner-iframe-icon" class="glyphicon glyphicon-refresh glyphicon-refresh-animate"></span>
        </div>
        <iframe id="pdf_iframe" class="block" src="" frameborder="0"></iframe>
        <div class="wait-message" id="status"></div>
      </div>
    </div>
  </div>
</div>
